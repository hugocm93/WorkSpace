package Mapa;

import Geometria.*;

public class Mundo {
	
		Territorio[] territorios;
		
		Tabela1 tabTroca;
		
		public Mundo(){
			territorios = new Territorio[1];
			territorios[0] = new Territorio();
			territorios[0].setNome("Brasil");
			territorios[0].setPoligono(new Poligono());
		}

		public Territorio getTerritorios(int i) {
			return territorios[i];
		}

		
		

}
