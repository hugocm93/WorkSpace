package Interface;

import java.awt.HeadlessException;
import java.awt.Graphics;
import java.awt.image.BufferedImage;
import java.io.File;
import java.io.IOException;
import javax.imageio.*;
import javax.swing.*;

public class Tabuleiro extends JFrame{
	
	
	public Tabuleiro(String title) throws HeadlessException {
		super(title);
		
		BufferedImage img = null;
		boolean booldrawImage;
		Graphics g;
		
		try {
		    img = ImageIO.read(new File("war_tabuleiro_com_nomes.png"));
		} 
		catch (IOException e) {
			System.out.println("Imagem n√£o encontrada.");
		}
		
		booldrawImage = g.drawImage(img, 0, 0, this);
		
		
	}
	
	

}
