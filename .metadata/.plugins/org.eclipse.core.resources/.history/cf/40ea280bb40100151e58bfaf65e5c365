package Mapa;

public class Tabela1 {
	
	private final int[] troca = {4,6,8,10,12,15,20,25,35,45};
	private int ultimaTroca=0;
	private int i = 0;
	
	public int proximoValor(){
		
		if(ultimaTroca < 9){
			i++;
			return troca[ultimaTroca++];
		}
		else{
			i++;
			return troca[ultimaTroca] + (i-9)*10;
		}
		
	}
	

}
